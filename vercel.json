{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://eaznexora.github.io/Ramadan-Time-Table/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
